{
  "$id": "https://yo-ai.ai/schemas/implementation-instances.search.output.schema.json",
  "title": "Implementation-Instances.Search.Output",
  "description": "Profile-aware output for the Implementation-Instances.Search capability.",
  "type": "object",
  "properties": {
    "result": {
      "type": "object",
      "description": "Primary capability output fields.",
      "properties": {
        "implementation_instances": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "asset_id": { "type": "string" },
              "location": { "type": "string" },
              "evidence": { "type": "string" }
            }
          }
        }
      }
    },
    "profile": {
      "$ref": "https://yo-ai.ai/schemas/profile.schema.json"
    },
    "profilePatch": {
      "$ref": "https://yo-ai.ai/schemas/profile.patch.schema.json"
    },
    "governanceLabels": {
      "$ref": "https://yo-ai.ai/schemas/governanceLabels.schema.json"
    }
  },
  "required": ["result"]
}