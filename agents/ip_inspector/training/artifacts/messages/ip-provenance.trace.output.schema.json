{
  "$id": "https://yo-ai.ai/schemas/ip-provenance.trace.output.schema.json",
  "title": "IP-Provenance.Trace.Output",
  "description": "Profile-aware output for the IP-Provenance.Trace capability.",
  "type": "object",
  "properties": {
    "result": {
      "type": "object",
      "description": "Primary capability output fields.",
      "properties": {
        "provenance": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "asset_id": { "type": "string" },
              "history": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "owner": { "type": "string" },
                    "from": { "type": "string", "format": "date" },
                    "to": { "type": "string", "format": "date" }
                  }
                }
              }
            }
          }
        }
      }
    },
    "profile": {
      "$ref": "https://yo-ai.ai/schemas/profile.schema.json"
    },
    "profilePatch": {
      "$ref": "https://yo-ai.ai/schemas/profile.patch.schema.json"
    },
    "governanceLabels": {
      "$ref": "https://yo-ai.ai/schemas/governanceLabels.schema.json"
    }
  },
  "required": ["result"]
}