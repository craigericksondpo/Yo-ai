{
  "$id": "https://yo-ai.ai/schemas/broker-evidence.collect.output.schema.json",
  "title": "Broker-Evidence.Collect.Output",
  "description": "Profile-aware output for the Broker-Evidence.Collect capability.",
  "type": "object",
  "properties": {
    "result": {
      "type": "object",
      "description": "Primary capability output fields.",
      "properties": {
        "evidence_id": {
          "type": "string",
          "description": "Unique identifier for the captured evidence record."
        },
        "status": {
          "type": "string",
          "description": "Status of the evidence capture process.",
          "enum": ["captured", "failed", "partial"]
        },
        "stored_metadata": {
          "type": "object",
          "description": "Structured metadata stored with the evidence.",
          "additionalProperties": true
        }
      },
      "required": ["evidence_id", "status"]
    },
    "profile": {
      "$ref": "https://yo-ai.ai/schemas/profile.schema.json"
    },
    "profilePatch": {
      "$ref": "https://yo-ai.ai/schemas/profile.patch.schema.json"
    },
    "governanceLabels": {
      "$ref": "https://yo-ai.ai/schemas/governanceLabels.schema.json"
    }
  },
  "required": ["result"]
}