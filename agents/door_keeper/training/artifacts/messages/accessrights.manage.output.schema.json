{
  "$id": "https://yo-ai.ai/schemas/accessrights.manage.output.schema.json",
  "title": "AccessRights.Manage.Output",
  "description": "Profile-aware output for the AccessRights.Manage capability.",
  "type": "object",
  "properties": {
    "result": {
      "type": "object",
      "description": "Primary capability output fields.",
      "properties": {
        "registration_identifier_ref": {
          "type": "string",
          "description": "Echo of the registration reference for traceability."
        },
        "finding": {
          "type": "string",
          "description": "Access decision or rationale (e.g., 'granted', 'denied', 'escalated')."
        },
        "log_refs": {
          "type": "array",
          "description": "Subset of logs used in the decision or flagged for review.",
          "items": { "type": "string" }
        }
      },
      "required": ["registration_identifier_ref", "finding", "log_refs"]
    },
    "profile": {
      "$ref": "https://yo-ai.ai/schemas/profile.schema.json"
    },
    "profilePatch": {
      "$ref": "https://yo-ai.ai/schemas/profile.patch.schema.json"
    },
    "governanceLabels": {
      "$ref": "https://yo-ai.ai/schemas/governanceLabels.schema.json"
    }
  },
  "required": ["result"]
}