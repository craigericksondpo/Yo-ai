{
  "$id": "https://yo-ai.ai/schemas/credentials.generate.output.schema.json",
  "title": "Credentials.Generate.Output",
  "description": "Profile-aware output for the Credentials.Generate capability.",
  "type": "object",
  "properties": {
    "result": {
      "type": "object",
      "description": "Primary capability output fields.",
      "properties": {
        "registration_identifier_ref": {
          "type": "string",
          "description": "Echo of the registration reference for traceability."
        },
        "finding": {
          "type": "string",
          "description": "Outcome of the credential generation process (e.g., 'issued', 'denied', 'escalated')."
        },
        "log_refs": {
          "type": "array",
          "description": "Logs used or generated during credential generation.",
          "items": { "type": "string" }
        }
      },
      "required": ["registration_identifier_ref", "finding", "log_refs"]
    },
    "profile": {
      "$ref": "https://yo-ai.ai/schemas/profile.schema.json"
    },
    "profilePatch": {
      "$ref": "https://yo-ai.ai/schemas/profile.patch.schema.json"
    },
    "governanceLabels": {
      "$ref": "https://yo-ai.ai/schemas/governanceLabels.schema.json"
    }
  },
  "required": ["result"]
}