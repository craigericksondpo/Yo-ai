{
  "$id": "https://yo-ai.ai/schemas/visitor.identify.output.schema.json",
  "title": "Visitor.Identify.Output",
  "description": "Profile-aware output for the Visitor.Identify capability.",
  "type": "object",
  "properties": {
    "result": {
      "type": "object",
      "description": "Primary capability output fields.",
      "properties": {
        "identifier_ref": {
          "type": "string",
          "description": "Echo of the visitor reference for traceability."
        },
        "finding": {
          "type": "string",
          "description": "Outcome of the visitor identification process (e.g., 'identified', 'unknown', 'escalated')."
        },
        "log_refs": {
          "type": "array",
          "description": "Logs used or generated during visitor identification.",
          "items": { "type": "string" }
        }
      },
      "required": ["identifier_ref", "finding", "log_refs"]
    },
    "profile": {
      "$ref": "https://yo-ai.ai/schemas/profile.schema.json"
    },
    "profilePatch": {
      "$ref": "https://yo-ai.ai/schemas/profile.patch.schema.json"
    },
    "governanceLabels": {
      "$ref": "https://yo-ai.ai/schemas/governanceLabels.schema.json"
    }
  },
  "required": ["result"]
}