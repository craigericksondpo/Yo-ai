{
  "$id": "https://yo-ai.ai/schemas/compliance-standard.get.output.schema.json",
  "title": "Compliance-Standard.Get.Output",
  "description": "Profile-aware output for the Compliance-Standard.Get capability.",
  "type": "object",
  "properties": {
    "result": {
      "type": "object",
      "description": "Primary capability output fields.",
      "properties": {
        "standard_ref": { "type": "string" },
        "title": { "type": "string" },
        "summary": { "type": "string" },
        "requirements": {
          "type": "array",
          "items": { "type": "string" }
        },
        "source_url": { "type": "string" }
      },
      "required": ["standard_ref", "summary"],
      "additionalProperties": false
    },
    "profile": {
      "$ref": "https://yo-ai.ai/schemas/profile.schema.json"
    },
    "profilePatch": {
      "$ref": "https://yo-ai.ai/schemas/profile.patch.schema.json"
    },
    "governanceLabels": {
      "$ref": "https://yo-ai.ai/schemas/governanceLabels.schema.json"
    }
  },
  "required": ["result"]
}