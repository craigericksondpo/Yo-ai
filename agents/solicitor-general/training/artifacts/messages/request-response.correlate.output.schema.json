{
  "$id": "https://yo-ai.ai/schemas/request-response.correlate.output.schema.json",
  "title": "Request-Response.Correlate.Output",
  "description": "Profile-aware output for the Request-Response.Correlate capability.",
  "type": "object",
  "properties": {
    "result": {
      "type": "object",
      "description": "Primary capability output fields.",
      "properties": {
        "correlationId": {
          "type": "string",
          "description": "Unique identifier for the correlation result."
        },
        "requestId": {
          "type": "string",
          "description": "Correlated request identifier."
        },
        "responseId": {
          "type": "string",
          "description": "Correlated response identifier."
        },
        "topic": {
          "type": "string",
          "description": "Topic associated with the correlation."
        },
        "status": {
          "type": "string",
          "enum": ["correlated", "not-found", "error"],
          "description": "Correlation status."
        },
        "error": {
          "type": "string",
          "description": "Error message if correlation failed."
        }
      },
      "required": ["correlationId", "status"],
      "additionalProperties": false
    },
    "profile": {
      "$ref": "https://yo-ai.ai/schemas/profile.schema.json"
    },
    "profilePatch": {
      "$ref": "https://yo-ai.ai/schemas/profile.patch.schema.json"
    },
    "governanceLabels": {
      "$ref": "https://yo-ai.ai/schemas/governanceLabels.schema.json"
    }
  },
  "required": ["result"]
}