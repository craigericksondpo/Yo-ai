{
  "$id": "https://yo-ai.ai/schemas/implementation-details.analyze.input.schema.json",
  "title": "Implementation-Details.Analyze.Input",
  "description": "Profile-aware input for the Implementation-Details.Analyze capability.",
  "type": "object",
  "properties": {
    "profile": {
      "$ref": "https://yo-ai.ai/schemas/profile.schema.json",
      "description": "Optional reference to the org or system profile to load."
    },
    "profilePatch": {
      "$ref": "https://yo-ai.ai/schemas/profile.patch.schema.json",
      "description": "Optional modifications to apply to the loaded profile."
    },
    "payload": {
      "type": "object",
      "description": "Capability-specific input fields for Implementation-Details.Analyze.",
      "properties": {
        "org_ref": {
          "type": "string"
        },
        "asset_names": {
          "type": "array",
          "items": { "type": "string" }
        }
      },
      "required": ["org_ref", "asset_names"]
    }
  },
  "required": ["payload"]
}
