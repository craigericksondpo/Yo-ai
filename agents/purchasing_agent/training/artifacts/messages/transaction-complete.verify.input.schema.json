{
  "$id": "https://yo-ai.ai/schemas/transaction-complete.verify.input.schema.json",
  "title": "Transaction-Complete.Verify.Input",
  "description": "Profile-aware input for the Transaction-Complete.Verify capability.",
  "type": "object",
  "properties": {
    "profile": {
      "$ref": "https://yo-ai.ai/schemas/profile.schema.json",
      "description": "Optional reference to the subject/org profile to load."
    },
    "profilePatch": {
      "$ref": "https://yo-ai.ai/schemas/profile.patch.schema.json",
      "description": "Optional modifications to apply to the loaded profile."
    },
    "payload": {
      "type": "object",
      "description": "Capability-specific input fields for Transaction-Complete.Verify.",
      "properties": {
        "provider": { "type": "string" },
        "referenceId": { "type": "string" }
      },
      "required": ["provider", "referenceId"]
    }
  },
  "required": ["payload"]
}