{
  "$id": "https://yo-ai.ai/schemas/dark-web.scan.output.schema.json",
  "title": "Dark-Web.Scan.Output",
  "description": "Output messageType for the Dark-Web.Scan capability.",
  "type": "object",
  "properties": {
    "scan_id": { "type": "string" },
    "results": {
      "type": "array",
      "description": "Matches found on dark web sources.",
      "items": {
        "type": "object",
        "required": ["source", "match_type", "evidence_hash"],
        "properties": {
          "source": { "type": "string" },
          "match_type": { "type": "string" },
          "evidence_hash": { "type": "string" },
          "listing_metadata": { "type": "object", "additionalProperties": true }
        }
      }
    },
    "risk_indicator": {
      "type": "string",
      "enum": ["low", "medium", "high", "critical"]
    }
  },
  "required": ["results", "scan_id"]
}
