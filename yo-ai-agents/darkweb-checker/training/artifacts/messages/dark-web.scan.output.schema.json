{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "DarkWebScanOutput",
  "type": "object",
  "required": ["results", "scan_id"],
  "properties": {
    "scan_id": { "type": "string" },
    "results": {
      "type": "array",
      "description": "Matches found on dark web sources.",
      "items": {
        "type": "object",
        "required": ["source", "match_type", "evidence_hash"],
        "properties": {
          "source": { "type": "string" },
          "match_type": { "type": "string" },
          "evidence_hash": { "type": "string" },
          "listing_metadata": { "type": "object", "additionalProperties": true }
        }
      }
    },
    "risk_indicator": {
      "type": "string",
      "enum": ["low", "medium", "high", "critical"]
    }
  }
}
