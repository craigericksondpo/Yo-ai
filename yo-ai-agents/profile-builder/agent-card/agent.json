      "inputSchema": {
        "type": "object",
        "properties": {
          "org_ref": { "type": "string" },
          "assets": { "type": "string" }
        },
        "required": ["org_ref", "assets"]
      },
      "outputSchema": {
        "type": "object",
        "properties": {
          "org_ref": { "type": "string" },
          "profile_url": { "type": "string" }
        }
      }
    }
  ],
  "supportsAuthenticatedExtendedCard": true
}
