{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "IdentifiabilityAssessInput",
  "type": "object",
  "required": ["dataset"],
  "properties": {
    "dataset": {
      "type": "array",
      "description": "Tabular dataset represented as array of objects.",
      "items": { "type": "object", "additionalProperties": true }
    },
    "quasi_identifiers": {
      "type": "array",
      "items": { "type": "string" }
    },
    "sensitive_attributes": {
      "type": "array",
      "items": { "type": "string" }
    }
  }
}
