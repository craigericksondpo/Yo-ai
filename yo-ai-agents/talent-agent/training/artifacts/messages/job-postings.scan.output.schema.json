{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "JobPostingsScanOutput",
  "type": "object",
  "required": ["scan_id", "postings"],
  "properties": {
    "scan_id": { "type": "string" },
    "postings": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["title", "company", "url"],
        "properties": {
          "title": { "type": "string" },
          "company": { "type": "string" },
          "url": { "type": "string" },
          "match_score": { "type": "number" }
        }
      }
    }
  }
}
